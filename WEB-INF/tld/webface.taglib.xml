<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
  
<facelet-taglib>

	<namespace>http://xmlns.idega.com/com.idega.webface</namespace>
	
	<tag>
		<tag-name>block</tag-name>
		<component>
            <component-type>WFBlock</component-type>
        </component>
	</tag>
	
	<tag>
		<tag-name>iwtree</tag-name>
		<component>
            <component-type>iwtree</component-type>
        </component>
	</tag>

	<tag>
		<tag-name>container</tag-name>
		<component>
            <component-type>WFContainer</component-type>
        </component>
	</tag>
	
</facelet-taglib>
