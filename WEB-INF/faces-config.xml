<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>

	<render-kit>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_container</renderer-type>
	          <renderer-class>com.idega.webface.redmond.ContainerRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_bezel</renderer-type>
	          <renderer-class>com.idega.webface.redmond.BezelRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_menu</renderer-type>
	          <renderer-class>com.idega.webface.redmond.MenuRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_block</renderer-type>
	          <renderer-class>com.idega.webface.redmond.BlockRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_titlebar</renderer-type>
	          <renderer-class>com.idega.webface.redmond.TitlebarRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_toolbar</renderer-type>
	          <renderer-class>com.idega.webface.redmond.ToolbarRenderer</renderer-class>
	     </renderer>
	     <renderer>
	          <component-family>webface</component-family>
	          <renderer-type>wf_htmlarea</renderer-type>
	          <renderer-class>com.idega.webface.htmlarea.HTMLAreaRenderer</renderer-class>
	     </renderer>
	</render-kit>


	<component>
		<component-type>WorkspacePage</component-type>
		<component-class>com.idega.webface.workspace.WorkspacePage</component-class>
	</component>

	<component>
		<component-type>WFBlock</component-type>
		<component-class>com.idega.webface.WFBlock</component-class>
	</component>
	
	<component>
		<component-type>WFSimpleDocumentsView</component-type>
		<component-class>com.idega.webface.WFSimpleDocumentsView</component-class>
	</component>
	
	<component>
		<component-type>WFWebView</component-type>
		<component-class>com.idega.webface.WFWebView</component-class>
	</component>	

	<component>
		<component-type>HTMLArea</component-type>
		<component-class>com.idega.webface.htmlarea.HTMLArea</component-class>
	</component>

	<managed-bean>
		<managed-bean-name>caseListBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.bean.CaseListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>fileUploadBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.bean.FileUploadBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>relatedItemsListBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.bean.CaseListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
</faces-config>
