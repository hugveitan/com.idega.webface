<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<faces-config>

	<component>
		<component-type>CMSPage</component-type>
		<component-class>com.idega.webface.test.CMSPage</component-class>
	</component>

	<component>
		<component-type>CreateArticlePage</component-type>
		<component-class>com.idega.webface.test.CreateArticlePage</component-class>
	</component>

	<component>
		<component-type>SearchArticlePage</component-type>
		<component-class>com.idega.webface.test.SearchArticlePage</component-class>
	</component>

	<component>
		<component-type>ListArticlesPage</component-type>
		<component-class>com.idega.webface.test.ListArticlesPage</component-class>
	</component>

	<component>
		<component-type>PreviewArticlePage</component-type>
		<component-class>com.idega.webface.test.PreviewArticlePage</component-class>
	</component>

	<managed-bean>
		<managed-bean-name>articleItemBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.ArticleItemBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>articleVersionItemBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.ArticleItemBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>previewArticleBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.ArticleItemBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>articleVersionListBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.ArticleVersionListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>articleListBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.ArticleListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>caseListBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.CaseListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>searchArticleBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.SearchArticleBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>listArticlesBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.ListArticlesBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>fileUploadBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.FileUploadBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>relatedItemsListBean</managed-bean-name>
		<managed-bean-class>com.idega.webface.test.CaseListBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
</faces-config>
